&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nb">cd&lt;/span> ~/Downloads
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">sudo tar xjf firefox-*.tar.bz2
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">sudo mv firefox /opt
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">sudo ln -s /opt/firefox/firefox /usr/local/bin/firefox
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">sudo wget https://raw.githubusercontent.com/mozilla/sumo-kb/main/install-firefox-linux/firefox.desktop -P /usr/local/share/applications
&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/div>&lt;ol start="2">
